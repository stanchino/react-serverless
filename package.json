{
  "name": "react-serverless-aws",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/react-serverless-aws"
  },
  "dependencies": {
    "@types/redux-actions": "^2.2.3",
    "@types/redux-form": "^7.0.12",
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.182.0",
    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-validators": "^2.3.2",
    "redux-saga": "^0.15.0",
    "redux-saga-routines": "^2.2.2"
  },
  "scripts": {
    "eslint": "node_modules/eslint/bin/eslint.js src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run eslint && react-scripts test --env=jsdom --coverage -u",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "redux-saga-testing": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/shared-examples.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": ["react-app"]
  }
}
